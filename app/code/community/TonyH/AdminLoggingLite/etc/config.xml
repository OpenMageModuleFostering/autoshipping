<config>
    <modules>
        <TonyH_AdminLoggingLite>
            <version>0.0.1</version>
        </TonyH_AdminLoggingLite>
    </modules>
    <global>
        <models>
            <tonyh_adminlogginglite>
                <class>TonyH_AdminLoggingLite_Model</class>
            </tonyh_adminlogginglite>
        </models>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <TonyH_AdminLoggingLite before="Mage_Adminhtml">TonyH_AdminLoggingLite_Adminhtml</TonyH_AdminLoggingLite>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>